<%- include('partials/header.ejs'); %>
<%- include('partials/menu.ejs', {userEmail: userEmail}); %>
<div>
    <% if (!userEmail) { %>
        <div class="login-card">
            <div class="fr-card">
                <div class="fr-card__body">
                    <div class="fr-card__content">
                        <form action="<%= loginUrl %>" method="get">

                            <label class="fr-label" for="email">Se logguer avec un email</label>
                            <input class="fr-input" type="email" id="email" name="email" required value="aaaa.yyyy@beta.gouv.fr">

                            <button class="fr-btn" type="submit">
                                Se connecter
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    <% } %>
</div>
<%- include('partials/footer.ejs') %>



